    <modelVersion>4.0.0</modelVersion>
    <groupId>com.hackerrank.sample</groupId>
    <artifactId>springboot_sample</artifactId>
    <version>1.0</version>
    <name>SpringBoot Base Project Template</name>

    <parent>
        <groupId>org.springframework.boot</groupId>
